{
  "$schema": "https://json-schema.org/schema",
  "$id": "library",
  "title": "Library generator",
  "description": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "private": {
      "type": "boolean",
      "description": "Is this library private?",
      "default": false
    }
  },
  "required": [
    "name"
  ]
}
